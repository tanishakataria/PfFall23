#include<stdio.h>
int main(){
int t;
    printf("enter the t in 24 hours");
    scanf("%d",&t);
    if(t>=5&&t<12)
    {
        printf("Good morning");
    }
   else if(t>=12&&t<18)
    {
        printf("Good evenng");
    }
     else  if (t>=18&&t<24)
     {
     printf("Good night");
}
    else {
    printf("unknown time");
    }
    return 0;
}
    
    
