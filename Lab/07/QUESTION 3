

#include <stdio.h>

int main()
{
    int N, d;
    printf("enter the size of array");
    scanf("%d",&N);
    int arr[N];
    printf("enter elements of array:\n");
    for(int i =0;i<N;i++){
        scanf("%d",&arr[i]);
    }
    printf("enter the number of position by left");
    scanf("%d",&d);
    d%=N;
    for(int i = 0;i<N;i++){
    printf("%d",arr[(i+d)%N]);
}
    return 0;
}
