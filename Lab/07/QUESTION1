#include <stdio.h>

int main()
{
    int x;
    int y;
    printf("enter the 1st number");
    scanf("%d",&x);
    printf("enter the second number");
    scanf("%d",&y);
    int i = 0;
     int ans = x;
    while(ans>0){
    ans = ans - y;
    i = i+1;
    }
    printf("\n %d divided by %d given: %d",x,y,i);
    return 0;
}
