
#include <stdio.h>
long int factorialnum(int N);

int main()
{
    int N;
    printf("enter a positive integer");
    scanf("%d",&N);
    printf("Factorial of %d = %ld",N,factorialnum(N));

    return 0;
}
long int factorialnum(int N){
   if(N>=1)
   return N*factorialnum(N-1);
   else
   return 1;
   
}
