
#include <stdio.h>
#include <string.h>

int main()
{

   char pass[20];
   printf("enter your password");
   scanf("%s",pass);
  if (strlen(pass)>=8){
      if (strcmp(pass,"secure123")==0){
          printf("login sucessful.welcome!\n");
      }
      else{
          printf("login failed.incorrect password");
      }
      }else { 
          printf("login failed.please must be atleast 8 character");
      }

  return 0;
}
